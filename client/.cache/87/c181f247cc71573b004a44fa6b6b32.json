 div.content {margin-left: 0;}\n}\n\n/* On screens that are less than 400px, display the bar vertically, instead of horizontally */\n@media screen and (max-width: 400px) {\n  .sidebar a {\n    text-align: center;\n    float: none;\n  }\n}\n.hr {\n  margin-top: 1rem;\n  margin-bottom: 1rem;\n  border: 0;\n  border-top: 1px solid rgba(0, 0, 0, 0.1);\n}\n.ujung {\n  margin-right: 20px\n}\n.ujung:last-child {\n  margin-right: 0px;\n}\n\n.searchBox {\n    top: 50%;\n    left: 50%;\n    background: #2f3640;\n    height: 61px;\n    border-radius: 40px;\n    padding: 10px;\n\n}\n\n.searchBox:hover > .searchInput {\n    width: 240px;\n    padding: 0 6px;\n}\n\n.searchBox:hover > .searchButton {\n  background: white;\n  color : #2f3640;\n}\n\n.searchButton {\n    color: white;\n    float: right;\n    width: 40px;\n    height: 40px;\n    border-radius: 50%;\n    background: #2f3640;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    transition: 0.4s;\n}\n\n.searchInput {\n    border:none;\n    background: none;\n    outline:none;\n    float:left;\n    padding: 0;\n    color: white;\n    font-size: 16px;\n    transition: 0.4s;\n    line-height: 40px;\n    width: 0px;\n\n}\n\n@media screen and (max-width: 620px) {\n.searchBox:hover > .searchInput {\n    width: 150px;\n    padding: 0 6px;\n}\n}\n.post {cursor: pointer;}\n#footer {\n  clear: both;\n  position: relative;\n  height: 200px;\n  margin-top: -200px;\n}"},"lineCount":192}},"error":null,"hash":"2abfa7def033bb6a91736eda0d188c57","cacheData":{"env":{}}}